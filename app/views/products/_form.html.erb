<%= render('errors', :object => @product) %>

<%= form_for @product do |f| %>
  <%= f.label :username %>
  <%= f.collection_select :user_id, User.order(:username),:id, :username %>

  <%= f.label :name %>
  <%= f.text_field :name %>

  <%= f.label :description %>
  <%= f.text_field :description %>

  <%= f.submit %>
<% end %>
